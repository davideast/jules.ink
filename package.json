{
  "name": "jules-ink",
  "version": "0.1.0",
  "private": false,
  "description": "Print physical labels of your AI coding sessions with Jules",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "jules-ink": "./dist/cli.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "assets",
    "ui/dist"
  ],
  "workspaces": [
    "ui"
  ],
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run build && npm run build:ui",
    "start": "tsx src/index.ts",
    "dev:ui": "npm run dev -w ui",
    "build:ui": "npm run build -w ui",
    "preview:ui": "npm run preview -w ui",
    "test": "vitest",
    "test:smoke": "vitest run tests/cli.smoke.test.ts",
    "gen:assets": "tsx scripts/create-base-template.ts"
  },
  "keywords": [
    "jules",
    "ai",
    "coding",
    "labels",
    "thermal-printer",
    "cli"
  ],
  "author": "David East",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davideast/jules.ink.git"
  },
  "dependencies": {
    "@google/genai": "^1.39.0",
    "@google/jules-sdk": "^0.0.4",
    "@hono/node-server": "^1.19.6",
    "@napi-rs/canvas": "^0.1.44",
    "@oslojs/encoding": "^1.1.0",
    "astro": "^5.17.1",
    "bottleneck": "^2.19.5",
    "clsx": "^2.1.1",
    "commander": "^14.0.3",
    "cookie": "^1.1.1",
    "cssesc": "^3.0.0",
    "devalue": "^5.6.2",
    "dotenv": "^16.4.5",
    "es-module-lexer": "^1.7.0",
    "gpt-tokenizer": "^3.4.0",
    "hono": "^4.10.6",
    "html-escaper": "^3.0.3",
    "micromatch": "^4.0.8",
    "mrmime": "^2.0.1",
    "ollama": "^0.6.3",
    "parse-diff": "^0.11.1",
    "piccolore": "^0.1.3",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-markdown": "^10.1.0",
    "send": "^1.2.1",
    "server-destroy": "^1.0.1",
    "sharp": "^0.34.5",
    "unstorage": "^1.17.4",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/micromatch": "^4.0.10",
    "@types/node": "^20.11.20",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3",
    "vitest": "^1.3.1"
  }
}
